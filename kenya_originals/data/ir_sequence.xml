<odoo>
	<data noupdate="1">

		<record id="res_partner_customer" model="ir.sequence">
			<field name="name">Business Partner Code Customer</field>
			<field name="code">res.partner.customer</field>
			<field name="prefix">CUS</field>
			<field name="active">TRUE</field>
			<field name="padding">3</field>
			<field name="number_next">1</field>
		</record>

		<record id="res_partner_supplier" model="ir.sequence">
			<field name="name">Business Partner Code Supplier</field>
			<field name="code">res.partner.supplier</field>
			<field name="prefix">SUP</field>
			<field name="active">TRUE</field>
			<field name="padding">3</field>
			<field name="number_next">1</field>
		</record>

		<record id="res_partner_employee" model="ir.sequence">
			<field name="name">Business Partner Code Employee</field>
			<field name="code">res.partner.employee</field>
			<field name="prefix">EMP</field>
			<field name="active">TRUE</field>
			<field name="padding">3</field>
			<field name="number_next">1</field>
		</record>

		<record id="res_partner_delivery_child" model="ir.sequence">
			<field name="name">Delivery Partner Sequence</field>
			<field name="code">res.partner.delivery.child</field>
			<field name="prefix">/</field>
			<field name="active">TRUE</field>
			<field name="padding">2</field>
			<field name="number_next">1</field>
		</record>

		<record id="product_default_code_raw_material" model="ir.sequence">
			<field name="name">Product Internal Reference Raw Material</field>
			<field name="code">product.raw.material</field>
			<field name="prefix">RM-</field>
			<field name="active">TRUE</field>
			<field name="padding">5</field>
			<field name="number_next">1</field>
		</record>

		<record id="product_default_code_packaging" model="ir.sequence">
			<field name="name">Product Internal Reference Raw Material</field>
			<field name="code">product.template.packaging</field>
			<field name="prefix">PM-</field>
			<field name="active">TRUE</field>
			<field name="padding">5</field>
			<field name="number_next">1</field>
		</record>

	</data>
</odoo>